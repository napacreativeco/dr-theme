// PURPLE: 530da3
// WHITE: eaeaea

{
    "workbench.colorCustomizations": {
        "activityBar.background": "#000000",
        // Title Bar
        "titleBar.activeBackground": "#67ed9d",
        "titleBar.activeForeground": "#362424",
        "editor.background": "#191919",
        // Sidebar
        "sideBar.background": "#060606",
        "sideBarSectionHeader.background": "#191919",
        "sideBar.modifiedResourceForeground": "#eaeaea",
        // Git
        "gitDecoration.modifiedResourceForeground": "#eaeaea",
        "gitDecoration.addedResourceForeground": "#eaeaea",
        "gitDecoration.untrackedResourceForeground": "#eaeaea",
        "gitDecoration.stageModifiedResourceForeground": "#eaeaea",
        "gitDecoration.deletedResourceForeground": "#eaeaea",
        "gitDecoration.renamedResourceForeground": "#eaeaea",
        "gitDecoration.activeForeground": "#eaeaea",
    },
    "editor.tokenColorCustomizations": {
        "variables": "#eaeaea",
        "comments": "#530da3",
        "types": "#9dc2ab",
        "strings": "#eaeaea",
        "functions": "#b3cfa6",
        "keywords": "#e0a3e0",
        "numbers": "#eaeaea",
        "textMateRules": [
            {
                "scope": "variable.name",
                "settings": {
                    "foreground": "#d3ecbb"
                }
            }
        ]
    }
}