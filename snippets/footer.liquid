<div class="footer"> 
    <div class="footer--wrapper">

        <div class="left">
            <div class="logo">
                <img src="{{ 'dais--full-logo.svg' | asset_url }}" width="100" height="100" loading="lazy" alt="Dais Records" />
            </div>
            <div class="links-one">
                <ul>
                    {% for link in linklists.footer-left.links %}
                        <li>
                            <a class="type-writer" href="{{ link.url }}">{{ link.title }}</a> 
                        </li>
                    {% endfor %}
                </ul>
            </div>
            <div class="links-two">
                <ul>
                    {% for link in linklists.footer-right.links %}
                        <li>
                            <a class="type-writer" href="{{ link.url }}">{{ link.title }}</a> 
                        </li>
                    {% endfor %}
                </ul>
            </div>
        </div>

        <div class="right">

        </div>

    </div>
</div>

