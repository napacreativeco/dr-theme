<!doctype html>
<!--[if lt IE 7]><html class="no-js lt-ie9 lt-ie8 lt-ie7" lang="en"> <![endif]-->
<!--[if IE 7]><html class="no-js lt-ie9 lt-ie8" lang="en"> <![endif]-->
<!--[if IE 8]><html class="no-js lt-ie9" lang="en"> <![endif]-->
<!--[if IE 9 ]><html class="ie9 no-js"> <![endif]-->
<!--[if (gt IE 9)|!(IE)]><!--> <html class="no-js"> <!--<![endif]-->  
<head>
  <meta charset="utf-8" />
  <!--[if IE]><meta http-equiv='X-UA-Compatible' content='IE=edge,chrome=1' /><![endif]-->

  <title>
  {{ page_title }}{% if current_tags %} &ndash; tagged "{{ current_tags | join: ', ' }}"{% endif %}{% if current_page != 1 %} &ndash; Page {{ current_page }}{% endif %}{% unless page_title contains shop.name %} &ndash; {{ shop.name }}{% endunless %}
  </title>

  {% if page_description %}
    <meta name="description" content="{{ page_description | escape }}" />
  {% endif %}

  <link rel="canonical" href="{{ canonical_url }}" />

  <meta name="viewport" content="width=device-width, initial-scale=1" />
  
  {% render 'open-graph-tags' %}
  {% render 'twitter-card' %}

  {{ content_for_header }}

  {{ 'style.scss.css' | asset_url | stylesheet_tag }}

  {{ 'index.css' | asset_url | stylesheet_tag }}

  <!--[if lt IE 9]>
  <script src="{{ '//html5shiv.googlecode.com/svn/trunk/html5.js' }}"></script>
  <![endif]-->

  <script src="{{ 'shopify_common.js' | shopify_asset_url }}" async></script>
  {% if template contains 'customers' %}  
    <script src="{{ 'customer_area.js' | shopify_asset_url }}" async></script>
  {% endif %}
  
  <script src="{{ 'shopify_common.js' | asset_url }}" async></script>

  <script src="{{ 'option_selection.js' | shopify_asset_url }}" async></script>

  <script>jQuery('html').removeClass('no-js').addClass('js');</script>

  <link rel="stylesheet" href="{{ 'swiper.css' | asset_url }}" />


</head>

<body id="{{ page_title | handle }}" class="{% if customer %}customer-logged-in {% endif %}template-{{ template | replace: '.', ' ' | truncatewords: 1, '' | handle }}" >
  
  <header>
    {% render 'navbar' %}
  </header>

  <main role="main">
    {{ content_for_layout }}
  </main>

  <footer>
    {% render 'footer' %}
  </footer>



  <script async src="https://code.jquery.com/jquery-3.6.4.min.js" integrity="sha256-oP6HI9z1XaZNBrJURtCoUT5SUnxFr8s3BzRl+cbzUq8=" crossorigin="anonymous"></script>
  <script async src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.11.5/gsap.min.js" integrity="sha512-cOH8ndwGgPo+K7pTvMrqYbmI8u8k6Sho3js0gOqVWTmQMlLIi6TbqGWRTpf1ga8ci9H3iPsvDLr4X7xwhC/+DQ==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
  <script async src="https://cdn.jsdelivr.net/npm/swiper@9/swiper-bundle.min.js"></script>

  <script src="{{ 'shop.js' | asset_url }}" async></script>
  <script src="{{ 'compiled.js' | asset_url }}" async></script>
</body>

</html>